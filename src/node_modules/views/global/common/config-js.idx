function globalsConfig(viewsUrl,mainjs,initData){
    var nodeModules = viewsUrl+"/node_modules"
    require.config({
        paths: @@include('./node_modules.json')
    });
    require.config({
        baseUrl : viewsUrl,
        paths:{
            "views/global" : "global"
        }
    });

    var __requireJS = function(path,basepath){
        basepath = basepath || nodeModules;
        document.write('<script src="'+basepath+path+'"><\/script>');
    }
    var jquery = "/jquery3.js";
    if(!Object.create){
        jquery = "/jquery1.js";
        __requireJS('/es5-shim.js');
        __requireJS('/es5-sham.js');
    }
    __requireJS('/polyfill.js');
    __requireJS(jquery);
    __requireJS('/migrate.js');
    __requireJS('/bootstrap.js');
    define("views/global/common/Model", {globalData:initData});
    window.globalData = initData;
    __requireJS('/Web_SDK_Base_v2.8.0.js',viewsUrl+'/global/nim/sdk/')
    __requireJS('/Web_SDK_NIM_v2.8.0.js',viewsUrl+'/global/nim/sdk/')
    __requireJS('/common.js',viewsUrl+'/global/nim/function/')

    require([mainjs]);
}


